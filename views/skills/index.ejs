<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Express Dev Skills</title>
</head>

<body>
    <h1>My Skills</h1>
    <ul>
        <% skills.forEach(function(s) { %>
            <li>
                <%= s.skill %>
                    -
                    <%= s.learning ? 'learning' : 'not learned' %>
            </li>
            <% }) %>
    </ul>
</body>

</html>